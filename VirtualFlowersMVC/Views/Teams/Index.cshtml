@model IEnumerable<Models.Team>

@{
    ViewBag.Title = "Teams";
}

<h2>Index</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.TeamId)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.TeamName)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.TeamId)
        </td>
        <td>
            @Html.ActionLink(item.TeamName, "Index", "Matches", new { TeamId = item.TeamId }, null)
        </td>
    </tr>
}

</table>
