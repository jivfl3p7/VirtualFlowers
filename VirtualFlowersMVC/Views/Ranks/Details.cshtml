@model IEnumerable<Models.Rank>

@{
    ViewBag.Title = "Ranking list";
}

<h2>Ranking list </h2>

<table class="table">
    <tr>
       <th>
            @Html.DisplayNameFor(model => model.RankPosition)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.TeamId)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Points)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.RankPosition)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TeamId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Points)
            </td>
        </tr>
    }

</table>
